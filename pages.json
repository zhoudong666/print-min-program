  "tabBar": {
    // 配置tabbar样式
    // 文字颜色
    "color": "#000",
    // 选中文字颜色
    "selectedColor": "#FF9966",
    // tabbar背景色
    "backgroundColor": "#FFFFFF",
    // tabbar边框色
    "borderStyle": "black",
    // tabbar位置 默认bottom 在移动端不生效，在小欧程序中，不显示图标
    // "position":"top",
    // list数组中存放tabbar的具体配置
