<view class="app-container bg-white fs-36 data-v-62edabea"><view class="params bg-white bd-radius-10 data-v-62edabea"><view class="params-set bd-bottom-solid-default-1 margin-top-20 margin-bottom-50 data-v-62edabea"><block qq:if="{{isCopyOrScan==='copy'}}"><view class="count-conainer params-set-item margin-bottom-50 data-v-62edabea"><text class="data-v-62edabea">份数</text><view class="count bd-solid-default-1 bd-radius-5 bg-numbtn margin-right-12 data-v-62edabea"><view data-event-opts="{{[['tap',[['reduce',['$0'],['fileNums']]]]]}}" class="count-btn reduce data-v-62edabea" bindtap="__e">-</view><view class="data-v-62edabea"><input class="count-input bg-white data-v-62edabea" type="number" maxlength="2" id="inputId" disabled="{{!isFirst}}" data-event-opts="{{[['input',[['inputChange',['$event']]]],['blur',[['inputChange',['$event']]]]]}}" value="{{fileNums}}" bindinput="__e" bindblur="__e"/></view><view data-event-opts="{{[['tap',[['plus',['$0'],['fileNums']]]]]}}" class="count-btn plus data-v-62edabea" bindtap="__e">+</view></view></view></block><block qq:for="{{setParams}}" qq:for-item="paramItem" qq:for-index="paramIndex" qq:key="paramIndex"><view class="data-v-62edabea"><view class="radio-container params-set-item margin-bottom-35 data-v-62edabea"><text class="data-v-62edabea">{{paramItem.tempType}}</text><radio-group class="radio-group data-v-62edabea" id="{{paramIndex}}" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><block qq:for="{{paramItem.items}}" qq:for-item="optItem" qq:for-index="optIndex" qq:key="tempCode"><label class="radio-item data-v-62edabea"><view class="radio-title data-v-62edabea">{{optItem.tempDesc}}</view><view class="data-v-62edabea"><radio value="{{optItem.tempCode}}" checked="{{!!optItem.chooseType}}" disabled="{{!isFirst}}" class="data-v-62edabea"></radio></view></label></block></radio-group></view></view></block></view><block qq:if="{{isCopyOrScan==='scan'}}"><view class="margin-bottom-35 data-v-62edabea"><text class="margin-right-20 data-v-62edabea">扫描文件名（选填）</text><view class="input-box data-v-62edabea"><input class="uni-input data-v-62edabea" maxlength="20" placeholder="输入扫描件命名，限字20" disabled="{{!isFirst}}" data-event-opts="{{[['input',[['onKeyInput',['$event']]]]]}}" value="{{scanFileName}}" bindinput="__e"/><block qq:if="{{showClearIcon}}"><text data-event-opts="{{[['tap',[['clearIcon',['$event']]]]]}}" class="uni-icon data-v-62edabea" bindtap="__e">x</text></block></view></view></block><button type="primary" disabled="{{isBtnDisabled}}" data-event-opts="{{[['tap',[['starthandle',['$event']]]]]}}" bindtap="__e" class="data-v-62edabea">{{''+(isFirst?"开始":"继续")+btnText+''}}</button><view class="succ-tip flex data-v-62edabea"></view><view class="buttom-btn-container flex flex-row-reverse data-v-62edabea"><view class="padding-20 width100 data-v-62edabea"><button type="primary" disabled="{{isFirst||isBtnDisabled}}" data-event-opts="{{[['tap',[['$goToUrl',['/pages/common/payPage']]]]]}}" bindtap="__e" class="data-v-62edabea">去支付</button></view></view></view></view>